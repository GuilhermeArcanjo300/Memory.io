{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["prettier"],
    "rules": {
        "no-plusplus": "off",
        "linebreak-style": "off",
        "indent": ["error", 4],
        "no-unused-vars": "warn"
    }
}
